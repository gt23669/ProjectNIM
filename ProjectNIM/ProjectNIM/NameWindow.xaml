<Window x:Class="ProjectNIM.NameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectNIM"
        mc:Ignorable="d"
          WindowStartupLocation="CenterScreen"
        Title="NameWindow" Height="200" Width="400">
    <Grid>
        <Label Content="You Have Selected a Number of Players Now Enter the Names" HorizontalAlignment="Left" Margin="24,10,0,0" VerticalAlignment="Top" Width="345"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="24,64,0,0" TextWrapping="Wrap" Text="Player1" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="24,92,0,0" TextWrapping="Wrap" Text="Player2" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <Button Content="Continue" HorizontalAlignment="Left" Margin="200,77,0,0" VerticalAlignment="Top" Width="75" Click="btn_SeconContClicked"/>

    </Grid>
</Window>
